<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jee="http://www.springframework.org/schema/jee"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/jee
	http://www.springframework.org/schema/jee/spring-jee-3.1.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd
	http://code.alibabatech.com/schema/dubbo          
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
	default-lazy-init="false">
	<!-- 系统 -->
	<dubbo:reference id="sysUserService" interface="com.cool.api.SysUserService" ></dubbo:reference>
	
	<dubbo:reference id="sysUserRoleService" interface="com.cool.api.SysUserRoleService" ></dubbo:reference>
	
	<dubbo:reference id="sysUserMenuService" interface="com.cool.api.SysUserMenuService" ></dubbo:reference>

	<dubbo:reference id="sysMenuService" interface="com.cool.api.SysMenuService"></dubbo:reference>
	
	<dubbo:reference id="sysDeptService" interface="com.cool.api.SysDeptService" ></dubbo:reference>
	
	<dubbo:reference id="sysRoleService" interface="com.cool.api.SysRoleService"></dubbo:reference>
	
	<dubbo:reference id="sysRoleMenuService" interface="com.cool.api.SysRoleMenuService"></dubbo:reference>
	
	<dubbo:reference id="sysDicService" interface="com.cool.api.SysDicService"></dubbo:reference>
	
	<dubbo:reference id="sysParamService" interface="com.cool.api.SysParamService"></dubbo:reference>	
	<!-- 标签Rpc -->
	<dubbo:reference id="tagRpcService" interface="com.cool.api.TagRpcService"></dubbo:reference>
</beans>